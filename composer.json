{
    "name": "bbc/programmes-caching-library",
    "description": "Caching for /programmes projects",
    "type": "library",
    "require": {
        "php": ">=7.1.0",
        "psr/cache": "^1.0",
        "symfony/cache": ">=2.0.0"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "BBC\\ProgrammesCachingLibrary\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\BBC\\ProgrammesCachingLibrary\\": "tests/"
        }
    }
}
